{
  "users": [{
    "name": "administrator",
    "password": "administrator",
    "tags": "administrator"
  }, {
    "name": "developer",
    "password": "developer",
    "tags": "developer"
  }],
  "vhosts": [{
    "name": "/"
  }],
  "permissions": [{
    "user": "administrator",
    "vhost": "/",
    "configure": ".*",
    "write": ".*",
    "read": ".*"
  }, {
    "user": "developer",
    "vhost": "/",
    "configure": "",
    "write": ".*",
    "read": ".*"
  }],
  "exchanges": [{
    "name": "music-exchange",
    "vhost": "/",
    "type": "direct",
    "durable": true,
    "auto_delete": false,
    "internal": false,
    "arguments": {}
  }],
  "queues": [{
    "name": "music-recognition-request-queue",
    "vhost": "/",
    "durable": false,
    "auto_delete": false,
    "arguments": {
      "x-message-ttl": 90000
    }
  },{
    "name": "music-new-song-request-queue",
    "vhost": "/",
    "durable": true,
    "auto_delete": false,
    "arguments": {}
  },{
    "name": "music-recognition-response-queue",
    "vhost": "/",
    "durable": false,
    "auto_delete": false,
    "arguments": {
      "x-message-ttl": 10000
    }
  },{
    "name": "music-new-song-response-queue",
    "vhost": "/",
    "durable": true,
    "auto_delete": false,
    "arguments": {}
  }],
  "bindings": [{
    "source": "music-exchange",
    "vhost": "/",
    "destination": "music-recognition-request-queue",
    "destination_type": "queue",
    "routing_key": "recognition-request",
    "arguments": {}
  },{
    "source": "music-exchange",
    "vhost": "/",
    "destination": "music-new-song-request-queue",
    "destination_type": "queue",
    "routing_key": "new-song-request",
    "arguments": {}
  },{
    "source": "music-exchange",
    "vhost": "/",
    "destination": "music-recognition-response-queue",
    "destination_type": "queue",
    "routing_key": "recognition-response",
    "arguments": {}
  },{
    "source": "music-exchange",
    "vhost": "/",
    "destination": "music-new-song-response-queue",
    "destination_type": "queue",
    "routing_key": "new-song-response",
    "arguments": {}
  }],
  "policies":[
    {
      "vhost":"/",
      "name":"ha-policy",
      "pattern":"",
      "definition": {
        "ha-mode":"all",
        "ha-sync-mode":"automatic",
        "ha-sync-batch-size":5
      }
    }
  ]
}
